<script type="text/javascript">
    
    $(document).ready(function() {

        document.getElementById("page-top").setAttribute("style", "font-family:<%= ThemeFeature.first.font_family %>;");
        $('a').attr("style", "font-family:<%= ThemeFeature.first.font_family %>;");
        $("span[class!='color_label']").attr("style", "font-family:<%= ThemeFeature.first.font_family %>;");
        $(".color_label").attr("style", "font-family: 'Lato','Helvetica Neue',Helvetica,Arial,sans-serif");
        $('h2').attr("style", "font-family:<%= ThemeFeature.first.font_family%>;color: #fff");
    var CSduration = 500;
    $('#color-switcher > ul > li').tooltip();
    
    $('#toggle-switcher').click(function(){
        if($(this).hasClass('opened')) {
            $(this).removeClass('opened');
            $(this).find('i').removeClass('fa-times');
            $(this).find('i').addClass('fa-gear');
            $('#color-switcher').animate({'right':'-231px'},CSduration);
        }
        else {
            $(this).addClass('opened')
            $(this).find('i').removeClass('fa-gear');
            $(this).find('i').addClass('fa-times');
            $('#color-switcher').animate({'right':'-10px'},CSduration);
        }
    }); 
    
    $('#color-switcher > ul > li').click(function() {
        $("body").removeClass();
        // $("body").addClass("color-" + $(this).attr("id"));

          $.ajax({
		    url: "/change_color",
		    type: "POST",
		    data: {color: $(this).attr("id")},
	
		    success: function(data){

		      $("body").addClass(data.class);
		    }
		  })
    });
    
    $('#page-boxed-toggle').click(function() {
        $('#page-wrapper').toggleClass("page-boxed");
        $(this).toggleClass("active");
        
        var owl = $(".owl-carousel").data('owlCarousel');
        owl.reinit();
        
    });
    
});
 function changeFont(str){


          $.ajax({
		    url: "/change_font",
		    type: "POST",
		    data: {font_family: str},
	
		    success: function(data){

		      document.getElementById("page-top").setAttribute("style", "font-family:"+data.font+";");
              $('a').attr("style", "font-family:"+data.font+";");
              $('h2').attr("style", "font-family:"+data.font+";color: #fff");
              $("span[class!='color_label']").attr("style", "font-family:"+data.font+";");
              $(".color_label").attr("style", "font-family:'Lato','Helvetica Neue',Helvetica,Arial,sans-serif");
		    }
		  })

        
    }
$('#theme_title_btn').click(function() {
    if($("#theme_title").val().trim() == ""){
        alert("Enter any title")
    }else{
        $.ajax({
            url: "/change_title",
            type: "POST",
            data: {title: $("#theme_title").val()},
    
            success: function(data){
                $("#top_title").text(data.title);
                $("#head_title").text(data.title);
            }
          })  
    }
});

</script>


